   "source": [
    "def Descending_Order(num):\n",
    "    l = []\n",
    "    for i in str(num):\n",
    "        l.append(i)\n",
    "    l.sort(reverse=True)\n",
    "    \n",
    "    out = \"\"\n",
    "    for j in l:\n",
    "        out = out + j\n",
    "    \n",
    "    return out"
   ]
